 177:../HostCommunication/src/EndpointRadarBase.c **** static uint16_t send_frame_data(uint8_t endpoint, Radar_Handle_t radar_driver,
 178:../HostCommunication/src/EndpointRadarBase.c ****                                 uint8_t wait)
 179:../HostCommunication/src/EndpointRadarBase.c **** {
 180:../HostCommunication/src/EndpointRadarBase.c ****     uint16_t error_code;
 181:../HostCommunication/src/EndpointRadarBase.c **** 
 182:../HostCommunication/src/EndpointRadarBase.c ****     Frame_Info_t frame_info;
 183:../HostCommunication/src/EndpointRadarBase.c **** 
 184:../HostCommunication/src/EndpointRadarBase.c ****     /* get frame data */
