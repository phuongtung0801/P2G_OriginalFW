 191:../HostCommunication/src/EndpointRadarBase.c ****     	static uint32_t transport_buffer[BSP_MAX_ADC_BUFFER_SIZE_BYTES/sizeof(uint32_t)];
 192:../HostCommunication/src/EndpointRadarBase.c **** 
 193:../HostCommunication/src/EndpointRadarBase.c ****         /* send message containing frame data */
 194:../HostCommunication/src/EndpointRadarBase.c ****         uint32_t num_samples;
 195:../HostCommunication/src/EndpointRadarBase.c ****         uint32_t data_size;
 196:../HostCommunication/src/EndpointRadarBase.c ****         uint8_t message_header[18];
 197:../HostCommunication/src/EndpointRadarBase.c ****         uint32_t sample_start = 0;
 198:../HostCommunication/src/EndpointRadarBase.c **** 
 199:../HostCommunication/src/EndpointRadarBase.c ****         uint32_t num_used_ant = 2;
 200:../HostCommunication/src/EndpointRadarBase.c **** 
 201:../HostCommunication/src/EndpointRadarBase.c ****         if (frame_info.adc_resolution == 12)
 202:../HostCommunication/src/EndpointRadarBase.c ****         {
 203:../HostCommunication/src/EndpointRadarBase.c ****         	/* Selection of Antenna to output is currently only supported for 12 Bit ADC resolution! *
 204:../HostCommunication/src/EndpointRadarBase.c ****         	if( frame_info.rx_mask == 1 )
 205:../HostCommunication/src/EndpointRadarBase.c **** 			{
 206:../HostCommunication/src/EndpointRadarBase.c **** 				sample_start = 0;
 207:../HostCommunication/src/EndpointRadarBase.c **** 				num_used_ant = 1;
 208:../HostCommunication/src/EndpointRadarBase.c **** 
 209:../HostCommunication/src/EndpointRadarBase.c **** 			}
 210:../HostCommunication/src/EndpointRadarBase.c **** 			else if( frame_info.rx_mask == 2 )
 211:../HostCommunication/src/EndpointRadarBase.c **** 			{
 212:../HostCommunication/src/EndpointRadarBase.c **** 				num_used_ant = 1;
 213:../HostCommunication/src/EndpointRadarBase.c **** 				sample_start = frame_info.num_chirps *
 214:../HostCommunication/src/EndpointRadarBase.c **** 						frame_info.num_samples_per_chirp *
 215:../HostCommunication/src/EndpointRadarBase.c **** 						num_used_ant *
 216:../HostCommunication/src/EndpointRadarBase.c **** 						(frame_info.data_format == RADAR_RX_DATA_REAL ? 1 : 2);
 217:../HostCommunication/src/EndpointRadarBase.c **** 			}
 218:../HostCommunication/src/EndpointRadarBase.c ****         }
 219:../HostCommunication/src/EndpointRadarBase.c ****         /* setup message header */
 220:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u8 (message_header,  0, MSG_FRAME_DATA);
 221:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u32(message_header,  1, frame_info.frame_number);
 222:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u32(message_header,  5, frame_info.num_chirps);
 223:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u8 (message_header,  9, num_used_ant);
 224:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u32(message_header, 10, frame_info.num_samples_per_chirp);
 225:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u8 (message_header, 14, frame_info.rx_mask);
 226:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u8 (message_header, 15, frame_info.data_format);
 227:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u8 (message_header, 16, frame_info.adc_resolution);
 228:../HostCommunication/src/EndpointRadarBase.c ****         wr_payload_u8 (message_header, 17, frame_info.interleaved_rx);
 229:../HostCommunication/src/EndpointRadarBase.c **** 
 230:../HostCommunication/src/EndpointRadarBase.c ****         /* calculate size of data */
 231:../HostCommunication/src/EndpointRadarBase.c ****         /* calculate total number of bits and divide by 8 to get number of
 232:../HostCommunication/src/EndpointRadarBase.c ****            bytes. Always round upwards */
 233:../HostCommunication/src/EndpointRadarBase.c ****         num_samples = frame_info.num_chirps *
 234:../HostCommunication/src/EndpointRadarBase.c ****                       frame_info.num_samples_per_chirp *
 235:../HostCommunication/src/EndpointRadarBase.c ****                       num_used_ant *
 236:../HostCommunication/src/EndpointRadarBase.c ****                       (frame_info.data_format == RADAR_RX_DATA_REAL ? 1 : 2);
 237:../HostCommunication/src/EndpointRadarBase.c **** 
 238:../HostCommunication/src/EndpointRadarBase.c ****         data_size = num_samples * frame_info.adc_resolution;
 239:../HostCommunication/src/EndpointRadarBase.c ****         data_size = (data_size >> 3) + ((data_size & 0x07) ? 1 : 0);
 240:../HostCommunication/src/EndpointRadarBase.c **** 
 241:../HostCommunication/src/EndpointRadarBase.c ****         /* send message header */
 242:../HostCommunication/src/EndpointRadarBase.c ****         protocol_send_header(endpoint, data_size + sizeof(message_header));
 243:../HostCommunication/src/EndpointRadarBase.c ****         protocol_send_payload(message_header, sizeof(message_header));
 244:../HostCommunication/src/EndpointRadarBase.c **** 
 245:../HostCommunication/src/EndpointRadarBase.c ****         if (frame_info.adc_resolution == 12)
 246:../HostCommunication/src/EndpointRadarBase.c ****         {
 247:../HostCommunication/src/EndpointRadarBase.c **** 			uint8_t *destPtr = (uint8_t *)&transport_buffer[0];
 248:../HostCommunication/src/EndpointRadarBase.c **** 			uint8_t *srcPtr  = (uint8_t *)frame_info.sample_data;
 249:../HostCommunication/src/EndpointRadarBase.c **** 
 250:../HostCommunication/src/EndpointRadarBase.c **** 			uint32_t channel_inc = frame_info.num_samples_per_chirp*sizeof(uint16_t);
 251:../HostCommunication/src/EndpointRadarBase.c **** 			uint32_t chirp_inc   = channel_inc * frame_info.num_rx_antennas * (frame_info.data_format == RAD
 252:../HostCommunication/src/EndpointRadarBase.c **** 
 253:../HostCommunication/src/EndpointRadarBase.c **** 			uint32_t processed_out_bytes = 0;
 254:../HostCommunication/src/EndpointRadarBase.c **** 
 255:../HostCommunication/src/EndpointRadarBase.c **** 			for(uint32_t chirp = 0; chirp < frame_info.num_chirps; chirp++)
 256:../HostCommunication/src/EndpointRadarBase.c **** 			{
 257:../HostCommunication/src/EndpointRadarBase.c **** 				for(uint32_t ant_idx= 0; ant_idx < frame_info.num_rx_antennas; ant_idx++)  // max Ant mask!
 258:../HostCommunication/src/EndpointRadarBase.c **** 				{
 259:../HostCommunication/src/EndpointRadarBase.c **** 					uint8_t mask = (1 << ant_idx);
 260:../HostCommunication/src/EndpointRadarBase.c **** 					if( (mask & frame_info.rx_mask) != 0 )
 261:../HostCommunication/src/EndpointRadarBase.c **** 					{
 262:../HostCommunication/src/EndpointRadarBase.c **** 						uint32_t num_chan_per_ant = (frame_info.data_format == RADAR_RX_DATA_REAL ? 1 : 2);
 263:../HostCommunication/src/EndpointRadarBase.c **** 						for(uint32_t ch_idx = 0; ch_idx < num_chan_per_ant; ch_idx++)
 264:../HostCommunication/src/EndpointRadarBase.c **** 						{
 265:../HostCommunication/src/EndpointRadarBase.c **** 							uint32_t in_offset = (chirp_inc * chirp) + (channel_inc*(ant_idx*num_chan_per_ant + ch_idx )
 266:../HostCommunication/src/EndpointRadarBase.c **** 							size_t out_offset = packing_helper_12Bit(destPtr, (uint16_t *)&srcPtr[in_offset], frame_info
 267:../HostCommunication/src/EndpointRadarBase.c **** 			
 268:../HostCommunication/src/EndpointRadarBase.c **** 							destPtr += out_offset;
 269:../HostCommunication/src/EndpointRadarBase.c **** 							processed_out_bytes += out_offset;
 270:../HostCommunication/src/EndpointRadarBase.c **** 						}
 271:../HostCommunication/src/EndpointRadarBase.c **** 					}
 272:../HostCommunication/src/EndpointRadarBase.c **** 				}
 273:../HostCommunication/src/EndpointRadarBase.c **** 			}
 274:../HostCommunication/src/EndpointRadarBase.c **** 			/* now send the packed data at once */
 275:../HostCommunication/src/EndpointRadarBase.c **** 			protocol_send_payload((uint8_t*)transport_buffer, data_size);
