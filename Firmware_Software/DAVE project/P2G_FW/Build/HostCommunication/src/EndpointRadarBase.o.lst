 149:../HostCommunication/src/EndpointRadarBase.c **** static size_t packing_helper_12Bit( uint8_t *p_dest, uint16_t *p_src, size_t numSamples)
 150:../HostCommunication/src/EndpointRadarBase.c **** {
 151:../HostCommunication/src/EndpointRadarBase.c **** 	/* numBytes needs to be devided by 2 */
 152:../HostCommunication/src/EndpointRadarBase.c **** 	if( (numSamples & 1) != 0)
 153:../HostCommunication/src/EndpointRadarBase.c **** 		return -1;
 154:../HostCommunication/src/EndpointRadarBase.c **** 
 155:../HostCommunication/src/EndpointRadarBase.c **** 	size_t   processed_output_bytes = 0;
 156:../HostCommunication/src/EndpointRadarBase.c **** 
 157:../HostCommunication/src/EndpointRadarBase.c **** 	while (numSamples >= 2)
 158:../HostCommunication/src/EndpointRadarBase.c **** 	{
 159:../HostCommunication/src/EndpointRadarBase.c **** 		/* pack two samples into one 32 bit word */
 160:../HostCommunication/src/EndpointRadarBase.c **** 		uint32_t packed_word;
 161:../HostCommunication/src/EndpointRadarBase.c **** 		packed_word  = *p_src++;
 162:../HostCommunication/src/EndpointRadarBase.c **** 		packed_word |= *p_src++ << 12;
 163:../HostCommunication/src/EndpointRadarBase.c **** 
 164:../HostCommunication/src/EndpointRadarBase.c **** 		/* write packed word into buffer */
 165:../HostCommunication/src/EndpointRadarBase.c **** 		wr_payload_u32(p_dest, 0, packed_word);
 166:../HostCommunication/src/EndpointRadarBase.c **** 		p_dest += 3;
 167:../HostCommunication/src/EndpointRadarBase.c **** 
 168:../HostCommunication/src/EndpointRadarBase.c **** 		numSamples -= 2;
 169:../HostCommunication/src/EndpointRadarBase.c **** 		processed_output_bytes += 3;
 170:../HostCommunication/src/EndpointRadarBase.c **** 	}
 171:../HostCommunication/src/EndpointRadarBase.c **** 
 172:../HostCommunication/src/EndpointRadarBase.c **** 	return processed_output_bytes;
 173:../HostCommunication/src/EndpointRadarBase.c **** }
